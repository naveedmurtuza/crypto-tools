<Window x:Class="CryptoTools.UI.Windows.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Password" Height="135" Width="367" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="Password:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,63,4,0" Name="okBtn" VerticalAlignment="Top" Width="75" Click="okBtn_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" IsCancel="True" Margin="185,63,0,0" Name="button2" VerticalAlignment="Top" Width="75" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="81,15,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="260" />
    </Grid>
</Window>
