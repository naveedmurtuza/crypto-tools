<Window x:Class="CryptoTools.UI.Windows.ExamineSSLWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ExamineSSLWindow" Height="236" Width="445" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="338*" />
            <ColumnDefinition Width="85*" />
        </Grid.ColumnDefinitions>
        <TextBox Height="23" HorizontalAlignment="Stretch" Name="txtUrl" VerticalAlignment="Center" Margin="10,5,0,5" />
        <Button Content="Go!" Grid.Column="1" Height="23" HorizontalAlignment="Right" Name="button1" VerticalAlignment="Center" Width="59" Margin="0,0,10,0" Click="GoBtnClicked"/>
        <TextBox Grid.Row="1" Height="141" HorizontalAlignment="Stretch" Name="txtTerminal" VerticalAlignment="Center" Grid.RowSpan="5" Grid.ColumnSpan="2" Margin="12,11" IsReadOnly="True" FontFamily="Consolas" TextWrapping="Wrap" />
    </Grid>
</Window>
